<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers List</title>
</head>
<body>
    <h1>Hello, <%= name %>!</h1>
    <p>show nums if ur name is ammar</p>
    <% if(name=="Ammar"){ %>
    <h2>Here are some numbers:</h2>
    <ul>
        <% numbers.forEach(function(num) { %>
            <li><%= num %></li>
        <% }); %>
    </ul>
    <% } %>
</body>
</html>
